{
  "manifest_version": 3,
  "name": "TabTime",
  "description": "Track your time spent on each tab",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "TabTime"
  },
  "permissions": ["activeTab", "tabs"],
  "background": {
    "service_worker": "background.js"
  }
}
